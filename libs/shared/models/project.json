{
  "root": "libs/shared/models",
  "sourceRoot": "libs/shared/models/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/models/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/models"],
      "options": {
        "jestConfig": "libs/shared/models/jest.config.js",
        "passWithNoTests": true
      }
    },
    "db-pull": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma db pull --schema ./src/prisma/schema.prisma",
        "cwd": "libs/shared/models"
      }
    },
    "migrate-save": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate save --experimental --schema ./src/prisma/schema.prisma",
        "cwd": "libs/shared/models"
      }
    },
    "migrate-up": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate up --experimental --schema ./src/prisma/schema.prisma",
        "cwd": "libs/shared/models"
      }
    },
    "gen-client": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma generate --schema ./src/prisma/schema.prisma",
        "cwd": "libs/shared/models"
      }
    }
  },
  "tags": []
}
